# vim :set filetype=taskrc:

# Default filter.
uda.taskwarrior-tui.task-report.next.filter=status:pending project.not:Doblo

# The task list is not circular; i.e. pressing 'k' on the first item doesn't
# move the selection to the bottom of the list.
uda.taskwarrior-tui.task-report.looping=false

